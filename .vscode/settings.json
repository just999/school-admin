{
  "editor.fontFamily": "Cascadia Code, CaskaydiaCove Nerd Font Propo, JetBrainsMono NF, Cascadia Code, Consolas, Operator Mono, MonoLisa, VictorMono Nerd Font Mono, Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": "'calt', 'ss01'",
  "editor.fontWeight": "400",
  "editor.fontSize": 11,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.hover.enabled": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.addMissingImports": "explicit"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  // "editor.fontFamily": "'FiraCode Nerd Font', 'JetbrainsMono Nerd Font', 'Hack Nerd Font', 'monospace'",
  // "editor.lineHeight": 1.5,
  "editor.smoothScrolling": true,
  // "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "workbench.iconTheme": "emoji-file-icons",
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "prettier.trailingComma": "es5",
  "prettier.arrowParens": "always",
  "editor.autoClosingBrackets": "always",
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "php",
    "blade",
    "ejs",
    "jinja",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "plaintext",
    "markdown",
    "vue",
    "liquid",
    "erb",
    "lang-cfml",
    "cfml",
    "HTML (EEx)",
    "HTML (Eex)",
    "plist"
  ],
  "[javascriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  },
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "build",
    "package.json",
    "public",
    "db.json",
    ".gitignore",
    "**/settings.json"
  ],
  "liveServer.settings.fullReload": true,
  "javascript.autoClosingTags": true,
  "html.autoClosingTags": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": ["dengan"],
  "todo-tree.tree.showBadges": false,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FDFEFF",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#00A36C",
      "bold": false,
      "italic": false,
      "fontWeight": 100,
      "fontSize": 24
    },
    {
      "tag": "?",
      "color": "#FDFEFF",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#bda800",
      "bold": true,
      "fontSize": 18,
      "fontWeight": 600,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#FDFEFF",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "#FFA500",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "rgb(8, 4, 31)",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#F5DEB3",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#eee",
      "bold": false,
      "italic": false
    }
  ],
  "workbench.colorCustomizations": {
    "activityBar.foreground": "#CBC3FF",
    "activityBar.background": "#0A0049",
    "activityBar.inactiveForeground": "#254397",
    "activityBarBadge.foreground": "#360040",
    "activityBarBadge.background": "#EA72FF",
    "statusBar.foreground": "#FFD0C5",
    "statusBar.background": "#9C422E",
    "statusBarItem.hoverBackground": "#603E17",
    "statusBarItem.remoteForeground": "#F1E585",
    "statusBarItem.remoteBackground": "#6E6202",
    "statusBar.debuggingForeground": "#060100",
    "statusBar.debuggingBackground": "#D37864"
  }
}
